{
  "compilerOptions": {
    "jsx": "react-jsx",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "target": "es5",
    "module": "commonjs",
    "experimentalDecorators": true,
    "sourceMap": false,
    "inlineSourceMap": true,
    "declaration": true,
    "esModuleInterop": true,
    "lib": ["es2019", "dom"]
  },
  "include": [
    "./src/**/*"
  ],
}
